    "eslint": "^8.31.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "prettier": "^2.8.4",
    "tsx": "^3.12.3",
    "typescript": "^4.9.5"
  }
}
